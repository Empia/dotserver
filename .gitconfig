[core]
  whitespace = fix,space-before-tab,tab-in-indent,trailing-space
  editor = vim
[apply]
  whitespace = fix
[color]
	ui = auto
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	grep = auto
[branch]
  autosetupmerge = true	
[merge]
	summary = true
[push]
	default = tracking
[alias]
  l = pull
  p = push
  ap = add -p
  all = add --all
  st = status
  ci = commit -v
  br = branch
  co = checkout
  df = diff
